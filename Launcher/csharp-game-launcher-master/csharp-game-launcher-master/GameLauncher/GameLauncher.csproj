<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>images\Icon.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="images\Icon.ico" />
    <None Remove="images\Launcher.jpg" />
    <None Remove="images\LauncherBackground.png" />
    <None Remove="Videos\v.mp4" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Videos\v.mp4">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <Resource Include="images\Icon.ico" />
    <Resource Include="images\Launcher.jpg" />
    <Resource Include="images\LauncherBackground.png" />
  </ItemGroup>

</Project>